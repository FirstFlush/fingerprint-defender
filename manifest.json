{
    "manifest_version": 3,
    "name": "Fingerprint Defender",
    "version": "0.1.0",
    "background": {
      "service_worker": "dist/background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["dist/content.js"],
        "run_at": "document_start"
      }
    ]
  }
  